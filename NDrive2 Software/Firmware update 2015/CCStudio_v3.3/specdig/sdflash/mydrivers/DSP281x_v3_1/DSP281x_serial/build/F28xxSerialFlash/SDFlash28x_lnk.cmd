MEMORY
{
PAGE 0 :
   PRAMM0     : origin = 0x000000, length = 0x000200
   PRAMH0     : origin = 0x3f8000, length = 0x001000
   RESET      : origin = 0x3fffc0, length = 0x000040
         
PAGE 1 : 
   DRAMM0      : origin = 0x000200, length = 0x000200

/* The first 80 words of RAMM1 (0x400 - 0x44F) are reserved for use by the
   ROM bootloader.  Do not link any initialized sections there.
*/
   RAMM1      : origin = 0x000400, length = 0x000400
   DRAMH0     : origin = 0x3f9000, length = 0x001000         
}
 
 
SECTIONS
{
/* Compiler defined sections */
   .text             : > PRAMH0,     PAGE = 0
   .cinit            : > PRAMM0,     PAGE = 0
   .const            : > PRAMM0,     PAGE = 0
   .econst           : > PRAMM0,     PAGE = 0
   .pinit            : > PRAMM0,     PAGE = 0  
   .reset            : > RESET,      PAGE = 0, type = DSECT
   .switch           : > PRAMM0,     PAGE = 0

   .bss              : > RAMM1,      PAGE = 1
   .ebss             : > RAMM1,      PAGE = 1
   .cio              : > RAMM1,      PAGE = 1
   .stack            : > DRAMM0,     PAGE = 1
   .sysmem           : > RAMM1,      PAGE = 1
   .esysmem          : > RAMM1,      PAGE = 1

/* User defined sections */
   ramfuncs          : > PRAMH0,     PAGE = 0

}
